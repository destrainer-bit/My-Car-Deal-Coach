{
  "functions": {
    "api/*.js": {
      "runtime": "@vercel/node@3.0.0"
    }
  },
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "build": {
    "env": {
      "NODE_VERSION": "20",
      "NPM_CONFIG_IGNORE_OPTIONAL": "false"
    }
  },
  "installCommand": "npm install --legacy-peer-deps"
}


